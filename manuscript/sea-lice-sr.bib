	number = {1},
	urldate = {2020-03-09},
	journal = {Renewable Energy},
	author = {Li, Gong and Shi, Jing and Zhou, Junyi},
	month = jan,
	year = {2011},
	keywords = {Adaptive linear element, Back propagation, Bayesian combination, Neural network, Radial basis function, Wind speed forecasting},
	pages = {352--359},
}

