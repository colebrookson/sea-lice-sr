@article{hart_spatial_2017,
	title = {The spatial scales of species coexistence},
	volume = {1},
	url = {www.nature.com/natecolevol},
	doi = {10.1038/s41559-017-0230-7},
	abstract = {T he coexistence of large numbers of species has long been considered a paradox 1. This conceptualization has motivated a generation of community ecologists to try to solve the problem of species coexistence, often beginning with the premise that environments are spatially homogeneous 2-6. After more than half a century of theoretical and empirical work, however, it seems clear that mechanisms operating in spatially homogeneous environments can account for the coexistence of some species 7-10 , but relatively few compared with the diversity we observe in nature. Meanwhile, observational evidence that spatial environmental heterogeneity supports the coexistence of large numbers of species , although not definitive, is difficult to ignore. Innumerable field studies over the past two centuries 11-13 , as well as clear differences in species' physiological tolerances 14-17 , demonstrate that different species have different environmental requirements and so tend to benefit from different locations in spatially heterogeneous environments 11. Indeed, ecologists readily accept that species from different bioclimatic zones coexist at geographical scales because of differences in environmental conditions; spatial environmental heterogeneity is likely to be similarly powerful in promoting coexistence over much smaller areas. We would argue then, that far from being a mystery, and despite relatively little formal attention, the stable coexistence of large numbers of species is best explained by mechanisms that rely on spatial environmental heterogeneity at some scale. If the coexistence of many species has a straightforward solution, is the problem of species coexistence resolved? We argue that this is not the case and that attention should turn to a deeper set of unre-solved questions about the scales at which species diversity is maintained. Environments are spatially and temporally variable at nearly all scales, but not all environmental variation is effective at stabilizing coexistence 18. Moreover, the biological processes that influence the ability of species to coexist stably-demographic stochasticity, species interactions and dispersal-are themselves scale-dependent. Yet the spatial scales of species coexistence have rarely been explicitly considered 19,20 and have not been empirically assessed. This is a considerable shortcoming of contemporary ecology, given that the patterns of diversity that ecologists are charged with explaining , and that conservation biologists are charged with protecting, are strongly and continuously scale-dependent 21. This is not to say that the influence of scale on patterns and processes in community ecology has been ignored 22. Indeed, there has been considerable recent progress in developing more sophisticated conceptualizations of, and explanations for, the scale-dependence of observed species diversity (for example descriptions of processes contributing to species-area relationships 23,24 and the scale-dependent partitioning of diversity 25,26). At the same time, ecologists have become increasingly aware that explanations for the diversity observed within any given area necessarily require consideration of processes operating on a range of different scales (most recently summarized by metacommunity concepts 27). However, these advances are rarely linked to recent, synthetic understanding of the mechanisms that stabilize coexistence 28-30 , or consider how these mechanisms change in strength as area increases, and so leave the scale-dependence of the maintenance of diversity unaddressed. Here we introduce a framework for understanding and quantifying the scales at which species diversity is maintained. In general , scale refers to the dimension of a phenomenon, and in this Perspective we are concerned with the spatial dimensions of stable species coexistence (defined in Box 1). Scale has several components , and the one of primary interest here is area, often referred to as spatial extent 31-33. We use area as a lens through which to understand how other components of scale such as the magnitude and grain (that is, the resolution of a phenomenon 31) of spatial environmental heterogeneity, and the distances over which species disperse, combine to determine the area required for stable species coexistence. That is, we are ultimately interested in the scale-dependence of the mechanisms that promote stable species coexistence, and how these determine changes in the number of coexisting species as spatial extent increases. Our assessment of the scale-dependence of species coexistence, which focuses on diversity maintenance within guilds of competing species, reveals important, unresolved questions about the nature of diversity maintenance on real landscapes. In contrast to recent theoretical advances, answering these questions is primarily an empirical frontier. Meeting the empirical challenge will advance our understanding of the maintenance of diversity in nature and could improve the community ecology's contribution to biodiversity conservation. Understanding how species diversity is maintained is a foundational problem in ecology and an essential requirement for the discipline to be effective as an applied science. Ecologists' understanding of this problem has rapidly matured, but this has exposed profound uncertainty about the spatial scales required to maintain species diversity. Here we define and develop this frontier by proposing the coexistence-area relationship-a real relationship in nature that can be used to understand the determinants of the scale-dependence of diversity maintenance. The coexistence-area relationship motivates new empirical techniques for addressing important, unresolved problems about the influence of demographic stochasticity, environmental heterogeneity and dispersal on scale-dependent patterns of diversity. In so doing, this framework substantially reframes current approaches to spatial community ecology. Quantifying the spatial scales of species coexistence will permit the next important advance in our understanding of the maintenance of diversity in nature, and should improve the contribution of community ecology to biodiversity conservation.},
	journal = {Nature Ecology \& Evolution},
	author = {Hart, Simon P and Usinowicz, Jacob and Levine, Jonathan M},
	year = {2017},
}

@article{grigoratou_trait-based_2019,
