@article{joly_effect_2005,
	title = {The effect of bovine tuberculosis and brucellosis on reproduction and survival of wood bison in {Wood} {Buffalo} {National} {Park}},
	volume = {74},
	issn = {00218790},
	url = {http://doi.wiley.com/10.1111/j.1365-2656.2005.00953.x},
	doi = {10.1111/j.1365-2656.2005.00953.x},
	abstract = {1 Wood bison (Bison bison athabascae L.) abundance in Wood Buffalo\${\textbackslash}backslash\$nNational Park, Canada, declined from an excess of 10 000 bison in\${\textbackslash}backslash\$nthe late 1960s to a low of 2200 bison in the late 1990s.\${\textbackslash}backslash\$n\${\textbackslash}backslash\$n2 Bovine tuberculosis (Mycobacterium bovis) and brucellosis (Brucella\${\textbackslash}backslash\$nabortus), were introduced to Wood Buffalo National Park in the late\${\textbackslash}backslash\$n1920s. As each of these pathogens has the potential to reduce survival\${\textbackslash}backslash\$nand reproduction in bison, they are suspected to have played a role\${\textbackslash}backslash\$nin the decline in bison abundance.\${\textbackslash}backslash\$n\${\textbackslash}backslash\$n3 We live-captured bison in the winters of 1997�2000, tested for\${\textbackslash}backslash\$ntuberculosis, brucellosis and pregnancy and released animals with\${\textbackslash}backslash\$nradio transmitters to evaluate survival.\${\textbackslash}backslash\$n\${\textbackslash}backslash\$n4 We found that bison that were positive for both diseases were less\${\textbackslash}backslash\$nlikely to be pregnant or to survive the winter than bison positive\${\textbackslash}backslash\$nfor one or neither disease. Further, in one population, bison that\${\textbackslash}backslash\$nwere tuberculosis-positive had a substantially lower pregnancy probability.\${\textbackslash}backslash\$n\${\textbackslash}backslash\$n5 Demonstrating a negative effect of diseases on survival and reproduction\${\textbackslash}backslash\$nis a necessary, but not sufficient, test of the role of diseases\${\textbackslash}backslash\$nin bison population decline.},
	number = {3},
	journal = {Journal of Animal Ecology},
	author = {Joly, Damien O. and Messier, François},
