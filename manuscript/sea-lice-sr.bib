	journal = {Physica A: Statistical Mechanics and its Applications},
	author = {Liu, Rong and Liu, Guirong},
	month = jun,
	year = {2019},
	keywords = {Extinction, Intraguild predation, Mixed functional responses, Persistence in mean, Stochastic food-web model},
	pages = {121782},
}

@article{li_dynamics_2019,
	title = {Dynamics of a stochastic {Holling}-{Tanner} predator–prey model},
	issn = {0378-4371},
	url = {http://www.sciencedirect.com/science/article/pii/S0378437119310131},
