@article{awender_stability_2021,
	title = {Stability of generalized ecological-network models},
	volume = {31},
	issn = {1054-1500},
	url = {https://aip.scitation.org/doi/full/10.1063/5.0029934},
	doi = {10.1063/5.0029934},
	abstract = {The stability of ecological networks of varying topologies and predator–prey relationships is explored by applying the concept of generalized modeling. The effects of omnivory, complexity, enrichment, number of top predators, and predatory response are discussed. The degree of omnivory plays a large role in governing web stability at steady state. Complexity as measured from connectance and network size is not a perfect indicator of stability; large, highly connected webs can be just as stable as smaller, less connected ones. Learning behavior as expressed in Holling’s type III predatory response is stabilizing for food webs and provides exceptions to the paradox of enrichment for some topologies.},
