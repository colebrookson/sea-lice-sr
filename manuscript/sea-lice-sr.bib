	author = {Baker, Rachel E. and Mahmud, Ayesha S. and Wagner, Caroline E. and Yang, Wenchang and Pitzer, Virginia E. and Viboud, Cecile and Vecchi, Gabriel A. and Metcalf, C. Jessica E. and Grenfell, Bryan T.},
