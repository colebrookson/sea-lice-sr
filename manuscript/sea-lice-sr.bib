@article{devriese_microplastic_2015,
	title = {Microplastic contamination in brown shrimp ({Crangon} crangon, {Linnaeus} 1758) from coastal waters of the {Southern} {North} {Sea} and {Channel} area},
	volume = {98},
	url = {https://www.sciencedirect.com/science/article/pii/S0025326X1500418X},
	doi = {10.1016/J.MARPOLBUL.2015.06.051},
	abstract = {This study assessed the capability of Crangon crangon (L.), an ecologically and commercially important crustacean, of consuming plastics as an opportunistic feeder. We therefore determined the microplastic content of shrimp in shallow water habitats of the Channel area and Southern part of the North Sea. Synthetic fibers ranging from 200μm up to 1000μm size were detected in 63\% of the assessed shrimp and an average value of 0.68±0.55microplastics/g w. w. (1.23±0.99microplastics/shrimp) was obtained for shrimp in the sampled area. The assessment revealed no spatial patterns in plastic ingestion, but temporal differences were reported. The microplastic uptake was significantly higher in October compared to March. The results suggest that microplastics {\textgreater}20μm are not able to translocate into the tissues.},
	number = {1-2},
	journal = {Marine Pollution Bulletin},
	author = {Devriese, Lisa I. and van der Meulen, Myra D. and Maes, Thomas and Bekaert, Karen and Paul-Pont, Ika and Frère, Laura and Robbens, Johan and Vethaak, A. Dick},
