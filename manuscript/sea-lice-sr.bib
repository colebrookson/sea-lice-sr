@article{krkosek_sea_2010,
	title = {Sea lice and salmon in {Pacific} {Canada}: {Ecology} and policy},
	volume = {8},
	issn = {15409295},
	url = {http://doi.wiley.com/10.1890/080097},
	doi = {10.1890/080097},
	abstract = {The spread of sea lice (Lepeophtheirus salmonis) from salmon farms\${\textbackslash}backslash\$nprobably contributes to declines of some native Pacific salmon\${\textbackslash}backslash\$npopulations. Migration normally protects juvenile wild Pacific salmon\${\textbackslash}backslash\$nfrom the marine ectoparasite in coastal waters by separating juvenile\${\textbackslash}backslash\$nsalmon from infected wild adults that are located offshore. Farmed\${\textbackslash}backslash\$nsalmon populations dwarf natural coastal host populations, particularly\${\textbackslash}backslash\$nin winter, leading to biomagnification of louse populations. By spring,\${\textbackslash}backslash\$nthere may be large numbers of lice on farmed salmon, and this is\${\textbackslash}backslash\$nassociated with recurrent parasite infestations of wild juvenile salmon\${\textbackslash}backslash\$nand depressed wild salmon stocks. Abiotic (eg temperature and salinity),\${\textbackslash}backslash\$nbiotic (eg predator abundance and food availability), and management (eg\${\textbackslash}backslash\$nperiodically emptying farms and applying chemical parasiticides) factors\${\textbackslash}backslash\$nare thought to mediate the louse threat, but none have been well\${\textbackslash}backslash\$nstudied. Policy is needed that protects undeveloped juvenile salmon\${\textbackslash}backslash\$nhabitats and that supports long-term study of salmon ecosystems, to\${\textbackslash}backslash\$nevaluate the sustainability of wild and farmed salmon.},
	number = {4},
	journal = {Frontiers in Ecology and the Environment},
	author = {Krkošek, Martin},
	month = may,
	year = {2010},
	pages = {201--209},
}

