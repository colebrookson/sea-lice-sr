	abstract = {History of species arrival can influence plant community assembly. In this issue of the Journal of Vegetation Science, Sarneel et al. show that the strength of such historical contingency, or priority effects, varies with soil moisture in riparian plants.We discuss this studywithin a theoretical framework describing howand when priority effects occur via destabilizing and equalizingmechanisms.},
	number = {4},
	author = {Fukami, Tadashi and Mordecai, Erin A. and Ostling, Annette},
	year = {2016},
	doi = {10.1111/jvs.12434},
}

