@article{fukami_commentary_2016,
	title = {{COMMENTARY} {A} framework for priority effects},
	volume = {27},
	url = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/jvs.12434},
	doi = {10.1111/jvs.12434},
